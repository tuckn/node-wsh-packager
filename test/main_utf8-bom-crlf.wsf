<package>
  <job id = "run">
    <script language="JScript" src="./WshModules/WshJScriptExtension/index.js"></script>
    <script language="JScript" src="./WshModules/WshUtil/index.js"></script>
    <script language="JScript" src="./WshModules/WshConstants/JScript.js"></script>
    <script language="JScript" src="./WshModules/WshPath/index.js"></script>
    <script language="JScript" src="./WshModules/WshFileSystem/index.js"></script>
    <script language="JScript" src="./WshModules/WshOS/index.js"></script>
    <script language="JScript" src="./WshModules/WshProcess/index.js"></script>
    <script language="JScript" src="./WshModules/WshFileSystemExtra/index.js"></script>
    <script language="JScript" src="./WshModules/WshCommander/index.js"></script>
    <script language="JScript" src="./WshModules/WshChildProcess/index.js"></script>
    <script language="JScript" src="./WshModules/WshLogWriter/index.js"></script>
    <script language="JScript" src="./WshModules/WshUsersVarManager/index.js"></script>
    <script language="JScript" src="./WshModules/WshZLIB/index.js"></script>
    <script language="VBScript" src="./WshModules/WshUtil/Functions.vbs"></script>
    <script language="VBScript" src="./WshModules/WshConstants/VBScriptMSO.vbs"></script>
    <script language="VBScript" src="./WshModules/WshExcel/Functions.vbs"></script>
    <script language="JScript" src="./WshModules/WshExcel/index.js"></script>
    <script language="JScript" src="./WshModules/WshGraphics/index.js"></script>
    <script language="JScript">
// requires
var util = Wsh.Util;
var path = Wsh.Path;
var process = Wsh.Process;

var mainCmds = {}; // 0: wscript.exe, 1: main.wsf, 2: <mainCmds>
var ERR_TITLE = 'Error in WshGraphics/main.wsf';
// Ⅱここに日本語を書いときますねⅡ

var exitProcess = function () { // {{{
  var cmdlist = Object.keys(mainCmds).join(', ');

  console.log('ERROR: Set a command.');
  console.log('where <command> is one of:');
  console.log('    ' + cmdlist);
  console.log('');
  console.log('process.argv: [' + process.argv.join(', ') + ']');
  console.log('');
  process.exit(1);
}; // }}}

// Set the main command specified by a user
var mainCmd;
if (process.argv.length >= 3 && process.argv[2].indexOf('-') === -1) {
  mainCmd = process.argv[2];
} else {
  mainCmd = 'run';
}

var matchedCmd = Object.keys(mainCmds).find(function (key) {
  return mainCmd === key;
});

if (matchedCmd === undefined) {
  exitProcess();
} else {
  mainCmds[matchedCmd]();
}
    </script>
  </job>
</package>
