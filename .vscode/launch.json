{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "minifyWsf",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\cli.js",
      "args": [
        "--wsf-path", "${workspaceFolder}\\test\\src\\Main.wsf",
        "--dest-dir", "${workspaceFolder}\\test\\dest",
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "packScripts",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\cli.js",
      "args": [
        "packScripts",
        "--wsf-path", "${workspaceFolder}\\test\\src\\Main.wsf",
        "--ignores", "Main.js",
        "--dest-dir", "${workspaceFolder}\\test\\dest",
      ]
    }
  ]
}